SYSMODULES="net base" # admin"
MODULES="ldap mysql  gitlab notebook proxy nginx owncloud" # dashboard # hub 

OUTERHOSTNAME="polc.elte.hu"
OUTERHOSTPORT="89"
if [[ -z "${OUTERHOSTPORT}" ]]; then
 OUTERHOST=$OUTERHOSTNAME
else
 OUTERHOST=$OUTERHOSTNAME":"$OUTERHOSTPORT
fi

INNERHOST="polc.elte.hu"

PREFIX=""
PROJECT=""


DOCKERIP="/var/run/docker.sock"
DOCKERPORT=""
DOCKERPROTOCOL="unix"
if [[ -z "${DOCKERPORT}" ]]; then
 DOCKERARGS="-H $DOCKERPROTOCOL://$DOCKERIP"
else
 DOCKERARGS="-H $DOCKERPROTOCOL://$DOCKERIP:$DOCKERPORT"
fi

ROOT="/srv/"$PREFIX
BUILDDIR=$ROOT"/build"
DISKIMG=""
