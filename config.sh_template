LOOPNO="/dev/loop3"
USRQUOTA=10G

SUBNET="172.20.0.0/16"

LDAPDOMAIN=$OUTERHOST
SMTP="smtp"
SMTPPORT=25
EMAIL=

DUMMYPASS=""

REWRITEPROTO=http

HUB_DEBUG=True

ERROR_LOG="error.log"
CHECK_LOG="check.log"	

