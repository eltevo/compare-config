version: "2"

networks:
  ##NETWORK##:
    external: true

volumes:
  ##PREFIX##-report-##POSTFIX##:
    external: true

services:
  ##KERNELGATEWAY##:
    container_name: ##KERNELGATEWAY##
    image: ##PREFIX##-kernelgateway-##POSTFIX##
    build:
      context: .
      dockerfile: ##KERNELGATEWAY_DOCKERFILE##
    networks:
      - ##NETWORK##
    environment:
      KG_ALLOW_ORIGIN: '*'
    volumes:
      - ##PREFIX##-report-##POSTFIX##:/home:ro
    restart: always

