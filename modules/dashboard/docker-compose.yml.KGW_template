---
version: "2"

networks:
  project_network:
    external: 
      name: ##NETWORK##

services:
  ##KERNELGATEWAY##:
    build:
      context: .
      dockerfile: ##KERNELGATEWAY_DOCKERFILE##
    networks:
      project_network:
    environment:
      KG_ALLOW_ORIGIN: '*'
    volumes:
      - ##VOLUME##:/srv/app/data/:ro
    restart: always

