
  location /hub {
        proxy_set_header Host $http_host;
        proxy_pass http://svc-##PREFIX##-hub;
        access_log      /var/log/nginx/hub.access.log;
        error_log       /var/log/nginx/hub.error.log;
  }

  location /accounts {
    proxy_set_header Host $http_host;
    proxy_pass http://svc-##PREFIX##-hub/accounts;
  }

 
  location /admin {
    proxy_set_header Host $http_host;
    proxy_pass http://svc-##PREFIX##-hub/admin;
  }


  location / {
    rewrite / ##REWRITEPROTO##://##FQDN##/hub permanent;
  }

 location /static/ {
    proxy_set_header Host $http_host;
    proxy_pass http://svc-##PREFIX##-hub/static/;
  }

