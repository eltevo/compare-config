
####### Hydra 
  location /hydra/ {
     ## hydra host --dangerous-force-http , otherwise https
       # set $s           ##PREFIX##-hydra;
       # proxy_pass       http://$s:4444/;
       proxy_pass       http://##PREFIX##-hydra:4444/;
  }

  location /consent/ {
       # set $s           ##PREFIX##-hydraconsent;
       # proxy_pass       http://$s/;
       proxy_pass       http://##PREFIX##-hydraconsent/;
  }
####### Hydra end
