echo "You can check the result under ##REWRITEPROTO##://##OUTERHOST##/${NB_URL}/report"

if [ ! $1 ]; then

        echo "No directory specified, so assuming this one"
        /opt/conda/bin/bokeh serve  ./ --prefix /${NB_URL}/report --allow-websocket-origin ##OUTERHOST## --port 9000

else

        /opt/conda/bin/bokeh serve  $1 --prefix /${NB_URL}/report --allow-websocket-origin ##OUTERHOST## --port 9000

fi

